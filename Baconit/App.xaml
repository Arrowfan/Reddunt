<Application x:Class="Baconit.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Baconit"
             RequestedTheme="Dark">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Microsoft.UI.Xaml\Package_Themes_Generic.xaml" />
                <ResourceDictionary Source="Microsoft.UI.Xaml\Package_Themes_rs1_themeresources.xaml" />
                <ResourceDictionary Source="Microsoft.UI.Xaml\Package_Themes_rs2_generic.xaml" />
                <ResourceDictionary Source="Microsoft.UI.Xaml\Package_Themes_rs2_themeresources.xaml" />

                <ResourceDictionary Source="Microsoft.UI.Xaml\Themes\Generic.xaml" />
                <ResourceDictionary Source="Microsoft.UI.Xaml\Themes\rs1_themeresources.xaml" />
                <ResourceDictionary Source="Microsoft.UI.Xaml\Themes\rs2_generic.xaml" />
                <ResourceDictionary Source="Microsoft.UI.Xaml\Themes\rs2_themeresources.xaml" />

                <ResourceDictionary Source="Themes\RedduntResource.xaml" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
